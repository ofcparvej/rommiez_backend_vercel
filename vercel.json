{
    "version": 2,
    "builds": [
     {
      "src": "index.js",
      "use": "@vercel/node",
      "config": { "includeFiles": ["dist/**"] }
     }
    ],
    "routes": [
     {
      "src": "/(.*)",
      "dest": "index.js"
     }
    ],
    "functions": {
        "api/index.js": {
          "runtime": "nodejs18.x"
        }
      }
   }